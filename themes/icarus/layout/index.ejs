<% page.posts.each(function(post) { %>
    <%- partial('common/article', { post: post, index: true }) %>
<% });
if (page.total > 1) { %>
    <nav id="page-nav">
        <%- paginator({
            prev_text: '&laquo; ' + __('上一页'),
            next_text: __('下一页') + ' &raquo;'
        }) %>
    </nav>
<% } %>